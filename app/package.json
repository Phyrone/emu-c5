{
	"name": "app",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prod": "vite build && vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"fmt": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"grpc:gen": " protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_opt=forceLong=bigint --ts_proto_opt=env=both --ts_proto_opt=oneof=unions-value --ts_proto_opt=esModuleInterop=true --ts_proto_opt=globalThisPolyfill=true --ts_proto_out=src/lib/grpc -I ../proto ../proto/*.proto"
	},
	"devDependencies": {
		"@bufbuild/protobuf": "^2.5.1",
		"@eslint/compat": "^1.2.9",
		"@eslint/js": "^9.27.0",
