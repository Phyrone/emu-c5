		"eslint-plugin-svelte": "^3.9.0",
		"globals": "^16.2.0",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.33.10",
		"svelte-check": "^4.2.1",
		"tailwindcss": "^4.1.8",
		"terser": "^5.40.0",
		"ts-proto": "^2.7.0",
