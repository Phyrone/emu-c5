{
  "name": "app",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "grpc:gen": " protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_opt=forceLong=bigint --ts_proto_opt=env=both --ts_proto_opt=oneof=unions-value --ts_proto_opt=esModuleInterop=true --ts_proto_opt=globalThisPolyfill=true --ts_proto_out=src/lib/grpc -I ../proto ../proto/*.proto"
  },
  "devDependencies": {
    "@bufbuild/protobuf": "^2.5.1",
    "@eslint/compat": "^1.2.9",
    "@eslint/js": "^9.27.0",
    "@sveltejs/adapter-node": "^5.2.12",
    "@sveltejs/kit": "^2.21.1",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.8",
    "@types/node": "^22.15.24",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-svelte": "^3.9.0",
    "globals": "^16.2.0",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "svelte": "^5.33.10",
    "svelte-check": "^4.2.1",
    "tailwindcss": "^4.1.8",
    "ts-proto": "^2.7.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.13.4",
    "@inlang/paraglide-js": "^2.0.13"
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977"
}
